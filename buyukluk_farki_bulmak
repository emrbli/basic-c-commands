#include<stdio.h>
#include<conio.h>
#include<locale.h>

main(){  //İki sayı arasındaki büyüklük farkını bulan program
	
	setlocale (LC_ALL, "Turkish");
	
int sayi1,sayi2;

printf("Birinci sayiyi girin: ");
scanf("%d",&sayi1);

printf("İkinci sayiyi girin:  ");
scanf("%d",&sayi2);

if(sayi1>sayi2)
	printf("birinci sayi ikinci sayidan büyüktür.");
	
	
else if(sayi2>sayi1)
	printf("ikinci sayi birinci sayidan büyüktür.");
	
else{

	printf("ikinci sayi ve birinci sayi eşittir.");	
}
	
getch();		
	
	
	
	
}
